<AnchorPane prefHeight="500" prefWidth="600.0" styleClass="score" stylesheets="@style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
    <children>
        <Button layoutX="497.0" layoutY="433.0" mnemonicParsing="false" onAction="#start" prefHeight="61.0" prefWidth="97.0" styleClass="button1" stylesheets="@style.css" text="New Round">
         <font>
            <Font name="Book Antiqua" size="14.0" />
         </font></Button>
      <Label fx:id="first" layoutX="238.0" layoutY="55.0" prefHeight="32.0" prefWidth="78.0" />
      <Label fx:id="second" layoutX="238.0" layoutY="133.0" prefHeight="32.0" prefWidth="78.0" />
      <Label fx:id="third" layoutX="238.0" layoutY="209.0" prefHeight="24.0" prefWidth="52.0" />
      <Label fx:id="fourth" layoutX="238.0" layoutY="291.0" prefHeight="24.0" prefWidth="52.0" />
      <Label fx:id="fifth" layoutX="238.0" layoutY="363.0" prefHeight="24.0" prefWidth="38.0" />
      <Label fx:id="sixth" layoutX="238.0" layoutY="439.0" prefHeight="24.0" prefWidth="38.0" />
      <Button fx:id="showLeaderboard" layoutX="151.0" layoutY="-2.0" mnemonicParsing="false" onAction="#leaderboard" prefHeight="32.0" prefWidth="317.0" styleClass="scoreButton" stylesheets="@style.css" text="Show Leaderboard">
         <font>
            <Font name="Book Antiqua" size="12.0" />
         </font></Button>
        <Button layoutX="14.0" layoutY="433.0" mnemonicParsing="false" onAction="#endGame" prefHeight="61.0" prefWidth="97.0" styleClass="button1" stylesheets="@style.css" text="End Game">
         <font>
            <Font name="Book Antiqua" size="14.0" />
         </font></Button>
    </children>
</AnchorPane>
