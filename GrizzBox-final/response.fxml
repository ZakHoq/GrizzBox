<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" styleClass="respond" stylesheets="@style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>

      <TextField fx:id="respond" layoutX="93.0" layoutY="332.0" prefHeight="79.0" prefWidth="301.0" promptText="Respond">
         <font>
            <Font name="Book Antiqua" size="16.0" />
         </font></TextField>
      <Button fx:id="submit" layoutX="415.0" layoutY="332.0" mnemonicParsing="false" onAction="#store" prefHeight="63.0" prefWidth="112.0" styleClass="buttonSubmit" stylesheets="@style.css" text="Submit">
         <font>
            <Font name="Book Antiqua" size="23.0" />
         </font></Button>
      <Text fx:id="prompt" layoutX="82.0" layoutY="80.0" wrappingWidth="322.5999999999999" />
      <Button fx:id="show" layoutX="443.0" layoutY="131.0" mnemonicParsing="false" onAction="#show" opacity="0.0" prefHeight="63.0" prefWidth="123.0" text="show prompt" />
      <Label fx:id="userprompt" layoutX="204.0" layoutY="163.0" prefHeight="18.0" prefWidth="144.0" text="User 1 respond">
         <font>
            <Font name="Book Antiqua" size="18.0" />
         </font></Label>

   </children>
</AnchorPane>
