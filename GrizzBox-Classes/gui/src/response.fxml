<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      
      <TextField fx:id="respond" layoutX="168.0" layoutY="175.0" promptText="respond" />
      <Button fx:id="submit" layoutX="318.0" layoutY="175.0" mnemonicParsing="false" onAction="#store" text="submit" />
      <Text fx:id="prompt" layoutX="254.0" layoutY="62.0" />
      <Button fx:id="show" layoutX="168.0" layoutY="52.0" mnemonicParsing="false" onAction="#show" text="show prompt" />

   </children>
</AnchorPane>
